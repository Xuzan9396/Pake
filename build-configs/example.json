{
  "name": "example",
  "url": "https://www.example.com/",
  "icon": "./icons/example.png",
  "width": 1200,
  "height": 800,

  "platforms": {
    "macos": {
      "enabled": true,
      "targets": ["universal", "apple", "intel"]
    },
    "windows": {
      "enabled": true,
      "targets": ["x64", "arm64"]
    },
    "linux": {
      "enabled": false,
      "targets": ["deb", "appimage"]
    }
  },

  "options": {
    "title": "Example App",
    "resizable": true,
    "fullscreen": false,
    "maximize": false,
    "hideTitleBar": false,
    "alwaysOnTop": false,
    "appVersion": "1.0.0",
    "darkMode": false,
    "disabledWebShortcuts": false,
    "activationShortcut": "",
    "userAgent": "",
    "showSystemTray": false,
    "systemTrayIcon": "",
    "useLocalFile": false,
    "multiArch": false,
    "debug": false,
    "inject": [],
    "proxyUrl": "",
    "installerLanguage": "en-US",
    "hideOnClose": false,
    "incognito": false,
    "wasm": false,
    "enableDragDrop": false,
    "keepBinary": false,
    "multiInstance": false,
    "startToTray": false
  }
}
